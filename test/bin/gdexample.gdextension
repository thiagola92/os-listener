[configuration]

entry_symbol = "initialize_extension"
compatibility_minimum = "4.4"
reloadable = true

[libraries]

macos.debug = "res://bin/liboslistener.macos.template_debug.framework"
macos.release = "res://bin/liboslistener.macos.template_release.framework"
ios.debug = "res://bin/liboslistener.ios.template_debug.xcframework"
ios.release = "res://bin/liboslistener.ios.template_release.xcframework"
windows.debug.x86_32 = "res://bin/liboslistener.windows.template_debug.x86_32.dll"
windows.release.x86_32 = "res://bin/liboslistener.windows.template_release.x86_32.dll"
windows.debug.x86_64 = "res://bin/liboslistener.windows.template_debug.x86_64.dll"
windows.release.x86_64 = "res://bin/liboslistener.windows.template_release.x86_64.dll"
linux.debug.x86_64 = "res://bin/liboslistener.linux.template_debug.x86_64.so"
linux.release.x86_64 = "res://bin/liboslistener.linux.template_release.x86_64.so"
linux.debug.arm64 = "res://bin/liboslistener.linux.template_debug.arm64.so"
linux.release.arm64 = "res://bin/liboslistener.linux.template_release.arm64.so"
linux.debug.rv64 = "res://bin/liboslistener.linux.template_debug.rv64.so"
linux.release.rv64 = "res://bin/liboslistener.linux.template_release.rv64.so"
android.debug.x86_64 = "res://bin/liboslistener.android.template_debug.x86_64.so"
android.release.x86_64 = "res://bin/liboslistener.android.template_release.x86_64.so"
android.debug.arm64 = "res://bin/liboslistener.android.template_debug.arm64.so"
android.release.arm64 = "res://bin/liboslistener.android.template_release.arm64.so"
web.debug.wasm32 = "res://bin/liboslistener.web.template_debug.wasm32.nothreads.wasm"
web.release.wasm32 = "res://bin/liboslistener.web.template_release.wasm32.nothreads.wasm"

[dependencies]
ios.debug = {
    "res://bin/libgodot-cpp.ios.template_debug.xcframework": ""
}
ios.release = {
    "res://bin/libgodot-cpp.ios.template_release.xcframework": ""
}